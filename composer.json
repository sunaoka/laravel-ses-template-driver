{
    "name": "sunaoka/laravel-ses-template-driver",
    "description": "Amazon SES template mail driver for Laravel.",
    "keywords": ["laravel", "aws", "ses", "sesv2", "mail", "driver"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "sunaoka",
            "email": "sunaoka@pocari.org"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-mbstring": "*",
        "aws/aws-sdk-php": "^3.245.0",
        "illuminate/console": "^10.0 || ^11.0 || ^12.0",
        "illuminate/mail": "^10.0 || ^11.0 || ^12.0"
    },
    "require-dev": {
        "larastan/larastan": "^2.9.11 || ^3.0.0",
        "laravel/pint": "^1.18.2",
        "orchestra/testbench": "^8.28.0 || ^9.6.1 || ^10.0.0"
    },
    "autoload": {
        "psr-4": {
            "Sunaoka\\LaravelSesTemplateDriver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sunaoka\\LaravelSesTemplateDriver\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "4.0-dev"
        },
        "laravel": {
            "providers": [
                "Sunaoka\\LaravelSesTemplateDriver\\SesTemplateTransportServiceProvider"
            ],
            "aliases": {
            }
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
